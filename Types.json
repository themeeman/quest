{
  "String": ".",
  "Integer": "^[0-9]+$",
  "Signed Integer": "^(\\+|-)[0-9]+$",
  "UserMention": "^<@!?[0-9]{18}>$",
  "RoleMention": "^<@&[0-9]{18}>$",
  "ChannelMention": "^<#[0-9]{18}>$",
  "Boolean": "^(true|false|yes|no|y|n)$",
  "Decimal": "^[+-]?([0-9]+([.][0-9]*)?|[.][0-9]+)$",
  "BigNumber": "^([+-]?([0-9]+([.][0-9]*)?|[.][0-9]+)){1}(e[+]?[0-9]+)?$",
  "Float": "^([+-]?([0-9]+([.][0-9]*)?|[.][0-9]+)){1}(e[+-]?[0-9]+)?$"
}